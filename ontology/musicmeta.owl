                <owl:someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MusicEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-meta/isRealisedIn"/>
