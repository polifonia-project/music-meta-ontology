                <owl:allValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-meta/FormType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-meta/hasInstrumentation"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Instrumentation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-meta/hasKey"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Key"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
