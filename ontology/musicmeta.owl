        <rdfs:label xml:lang="en">Is part of release</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-meta/isProducedByRecordingProcess -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-meta/isProducedByRecordingProcess">
